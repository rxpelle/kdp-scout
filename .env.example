# KDP Scout Configuration
# Copy this file to .env and fill in your values

# DataForSEO API credentials (optional, for enhanced reverse ASIN)
# Sign up at https://dataforseo.com/
# DATAFORSEO_LOGIN is your account email
# DATAFORSEO_API_KEY is your API password/key
DATAFORSEO_LOGIN=
DATAFORSEO_API_KEY=

# Proxy URL for web scraping (optional)
PROXY_URL=

# Rate limits (seconds between requests)
AUTOCOMPLETE_RATE_LIMIT=0.5
PRODUCT_SCRAPE_RATE_LIMIT=2.0
SEARCH_PROBE_RATE_LIMIT=2.0
DATAFORSEO_RATE_LIMIT=0.5

# Database path (relative to project root)
DB_PATH=data/kdp_scout.db

# Logging level
LOG_LEVEL=INFO
